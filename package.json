{
  "name": "security-api",
  "version": "1.0.0",
  "description": "Restaurant security api",
  "scripts": {
    "prestart": "babel-node src/startMessage.js",
    "start": "nodemon src/server.js --exec babel-node -r dotenv/config --extensions \".ts,.js\"",
    "test": "mkdirp report/test -p && cross-env NODE_ENV=test NODE_CONFIG_DIR=./src/config SECURITY_ACCESS_TOKEN_SECRET=abc SECURITY_REFRESH_TOKEN_SECRET=abc babel-node --extensions '.js,.ts,.jsx,.tsx' runNodeTests.js src/**/*.test.js | tee report/test/test.out.tap | faucet && npm run tap-to-junit",
    "tap-to-junit": "tap-junit -o report/junit -n test.xml < report/test/test.out.tap",
    "build-dev": "babel src -d build --copy-files",
    "build": "webpack",
    "serve": "node securityApp.js",
    "debug": "babel-node -r dotenv/config --extensions \".ts,.js\" --nolazy --inspect-brk=9229 src/server.js",
    "migrate-dev": "cross-env NODE_ENV=development npm run migrate",
    "migrate-status-dev": "cross-env NODE_ENV=development npm run migrate-status",
    "migrate": "migrate-mongo up",
    "migrate-status": "migrate-mongo status",
    "proto-ts": "bash proto-to-ts.sh",
    "check-types": "tsc"
  },
  "author": "Marvin Andia",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/body-parser": "^1.19.0",
    "@types/config": "0.0.36",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/express-serve-static-core": "^4.17.7",
    "@types/google-protobuf": "^3.7.2",
    "@types/jaeger-client": "^3.15.4",
    "@types/mongoose": "^5.7.21",
    "@types/qs": "^6.9.3",
    "@types/redis": "^2.8.21",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.20.2",
    "faucet": "0.0.1",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^2.5.11",
    "migrate-mongo": "^7.2.1",
    "mkdirp": "^0.5.5",
    "nodemon": "^2.0.4",
    "proxyquire": "^1.8.0",
    "sinon": "^6.3.5",
    "supertest": "^3.4.2",
    "tap-junit": "^3.1.2",
