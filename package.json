{
  "name": "security-api",
  "version": "1.0.0",
  "description": "Restaurant security api",
  "scripts": {
    "prestart": "babel-node src/startMessage.js",
    "start": "nodemon src/server.js --exec babel-node -r dotenv/config --extensions \".ts,.js\"",
    "test": "mkdirp report/test -p && cross-env NODE_ENV=test NODE_CONFIG_DIR=./src/config SECURITY_ACCESS_TOKEN_SECRET=abc SECURITY_REFRESH_TOKEN_SECRET=abc babel-node --extensions '.js,.ts,.jsx,.tsx' runNodeTests.js src/**/*.test.js | tee report/test/test.out.tap | faucet",
    "tap-to-junit": "tap-junit -o report/junit -n test.xml < report/test/test.out.tap",
    "build-dev": "babel src -d build --copy-files",
    "build": "webpack --color",
    "serve": "node securityApp.js",
    "debug": "babel-node -r dotenv/config --extensions \".ts,.js\" --nolazy --inspect-brk=9229 src/server.js",
    "migrate-dev": "cross-env NODE_ENV=development npm run migrate",
    "migrate-status-dev": "cross-env NODE_ENV=development npm run migrate-status",
    "migrate": "migrate-mongo up",
    "migrate-status": "migrate-mongo status",
    "proto-ts": "bash proto-to-ts.sh",
    "check-types": "tsc",
    "stop": "pkill --signal SIGINT restaurant-security",
    "lint": "eslint src/**"
  },
  "author": "Marvin Andia",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-export-namespace-from": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.41",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/google-protobuf": "^3.15.6",
    "@types/jaeger-client": "^3.18.3",
    "@types/mongoose": "^5.11.97",
    "@types/qs": "^6.9.7",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.1.1",
    "faucet": "0.0.1",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2",
    "migrate-mongo": "^9.0.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.16",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0",
    "supertest": "^6.2.3",
    "tap-junit": "^5.0.2",
    "tape": "^5.5.3",
    "tape-promise": "^4.0.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.100.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.18.3",
    "base64url": "^3.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "colors": "^1.4.0",
    "config": "^3.3.7",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "google-protobuf": "^3.20.1",
    "grpc": "^1.24.11",
    "jaeger-client": "^3.19.0",
    "jwt-simple": "^0.5.6",
