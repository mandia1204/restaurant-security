{
  "name": "security-api",
  "version": "1.0.0",
  "description": "Restaurant security api",
  "scripts": {
    "prestart": "babel-node src/startMessage.js",
    "start": "nodemon src/server.js --exec babel-node -r dotenv/config",
    "test": "mkdirp report/test -p && cross-env NODE_CONFIG_DIR=./src/config SECURITY_ACCESS_TOKEN_SECRET=abc SECURITY_REFRESH_TOKEN_SECRET=abc babel-tape-runner src/**/*.test.js | tee report/test/test.out.tap | faucet",
    "build-dev": "babel src -d build --copy-files",
    "build": "webpack",
    "serve": "node securityApp.js",
    "debug": "babel-node --nolazy --inspect-brk=9229 src/services/tokenService.test.js",
		"mongo": "cross-env NODE_ENV=development migrate-mongo status"
  },
  "author": "Marvin Andia",
  "license": "ISC",
