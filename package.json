    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "faucet": "0.0.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.18.11",
    "proxyquire": "^1.8.0",
    "sinon": "^6.1.3",
    "supertest": "^3.0.0",
    "tape": "^4.10.1",
    "tape-promise": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
