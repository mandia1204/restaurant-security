    "colors": "^1.4.0",
    "config": "^3.3.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "google-protobuf": "^3.12.1",
    "grpc": "^1.24.2",
    "jaeger-client": "^3.18.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.26.0",
    "mongoose": "^5.9.15",
